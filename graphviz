digraph G {
	n2 [shape=record, label="<f> 2 | { Alternatives | 1 + 1 }"];
	n3 [shape=record, label="<f>3 | { Alternatives | { <e2> 2 | + | 1 } }"]
	n4 [shape=record, label="<f>4 | { Alternatives | { <e3> 3 | + | 1 } | { <e2>2 | + | <ee2> 2 } }"]
	n5 [shape=record, label="<f>5 | { Alternatives | { <e4> 4 | + 1 } |  { <e3> 3 | + | <e2> 2 } }"]

	n3:e2 -> n2:f;
	n4:e3 -> n3:f;
	n4:e2 -> n2:f;
	n4:ee2 -> n2:f;
	n5:e2 -> n2:f;
	n5:e3 -> n3:f;
	n5:e4 -> n4:f;
   }